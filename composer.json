{
  "name": "bankiru/yii-gelf",
  "type": "yii-extension",
  "description": "Graylog2 log route for Yii",
  "keywords": ["yii", "extension", "log", "log route", "graylog", "graylog2", "gelf"],
  "homepage": "https://github.com/bankiru/yii-gelf",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Boris Gorbylev",
      "email": "ekho@ekho.name",
      "homepage": "http://github.com/ekho/"
    }
  ],
  "require": {
    "php": "~5.4",
    "graylog2/gelf-php": "~1.2",
    "yiisoft/yii": "~1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.6",
    "fabpot/php-cs-fixer": "~1.8",
    "satooshi/php-coveralls": "~0.6"
  },
  "autoload": {
    "psr-4": {
      "Bankiru\\Yii\\Logging\\Graylog2\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bankiru\\Yii\\Logging\\Graylog2\\": "tests"
    }
  }
}
